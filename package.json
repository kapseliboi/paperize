{
  "name": "editor",
  "version": "0.0.5",
  "description": "The Paperize serverless web application",
  "main": "lib/main.js",
  "scripts": {
    "clean": "rm -rf build",
    "start": "concurrently \"npm run webpack-watch\" \"npm run server\"",
    "start-no-sync": "concurrently \"npm run webpack-watch -- --env.NO_SYNC=true\" \"npm run server\"",
    "server": "http-server build",
    "webpack-watch": "webpack --watch --mode=development",
    "dev-cypress": "NODE_ENV=test concurrently --kill-others \"npm run start-no-sync\" \"cypress open\"",
    "dev-mocha": "NODE_ENV=test concurrently --kill-others \"webpack --config webpack.test.config.js --watch\" \"mocha test/build/test.js --watch\"",
    "test-unit": "NODE_ENV=test webpack --config webpack.test.config.js && mocha test/build/test.js",
    "build-test-integration": "npm run clean && NODE_ENV=test webpack --mode=development",
    "test-integration": "npm run build-test-integration && concurrently --kill-others \"npm run server\" \"cypress run\"",
    "test": "npm run test-unit && npm run test-integration",
    "create-video": "npm run build-test-integration && concurrently --kill-others \"npm run server\" \"cypress run --config video=true\"",
    "copy-video": "cp cypress/videos/* build/releases/10-14-17.mp4",
    "finalize-prod": "coffee finalize.coffee",
    "build-prod": "npm run clean && webpack --mode=production",
    "deploy-prod": "coffee deploy.coffee",
    "bump-prod": "npm run build-prod && npm run deploy-prod",
    "release": "npm test && npm run build-prod && npm run deploy-prod",
    "old-release": "npm run finalize-prod && npm run create-video && npm run build-prod && npm run copy-video && npm run deploy-prod",
    "clean-screenshots": "rm -rf cypress/screenshots/documentation_screenshots_spec.coffee",
    "generate-screenshots": "npm run build-test-integration && concurrently --kill-others \"npm run server\" \"cypress run --spec cypress/integration/documentation_screenshots_spec.coffee\"",
    "copy-screenshots": "cp cypress/screenshots/documentation_screenshots_spec.coffee/*.png ../docs/static/generated-images",
    "bump-screenshots": "npm run clean-screenshots && npm run generate-screenshots && npm run copy-screenshots"
  },
  "author": "Loren Norman <loren@oilandrope.com>",
  "license": "ISC",
  "devDependencies": {
    "@mdi/font": "^3.4.93",
    "@mdi/js": "^3.4.93",
    "@vue/test-utils": "^1.0.0-beta.25",
    "ajv": "^5.2.3",
    "aws-sdk": "^2.88.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-bluebird": "^1.0.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.2.0",
    "cloc": "^2.3.4",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.0.0",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.5.1",
    "core-util-is": "^1.0.2",
    "css-loader": "^0.26.2",
    "cypress": "^3.4.1",
    "eslint": "^4.18.1",
    "eslint-plugin-vue": "^4.3.0",
    "express": "^4.14.1",
    "file-api": "^0.10.4",
    "file-loader": "^3.0.1",
    "http-server": "^0.10.0",
    "leveldown": "^3.0.2",
    "mime-types": "^2.1.14",
    "mocha": "^5.2.0",
    "moment-locales-webpack-plugin": "^1.0.7",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "recursive-readdir": "^2.2.1",
    "semver": "^5.3.0",
    "shelljs": "^0.8.2",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "util-deprecate": "^1.0.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
